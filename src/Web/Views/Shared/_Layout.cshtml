<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Web</title>
</head>
<body>
    <header></header>
    <main>
        @RenderBody()
    </main>
    @if (IsSectionDefined("LeftPanel"))
    {
        <aside>
            @RenderSection("LeftPanel", required: false)
        </aside>
    }
    <footer></footer>
</body>
</html>
